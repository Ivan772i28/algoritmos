Proceso numerosma
	Definir n1,n2,n3 Como Entero;
	Escribir "ingresa un n1";
	Leer n1;
	Escribir "ingresa un n2";
	Leer n2;
	Escribir "ingresa un n3";
	Leer n3;
	
		
		Si n1 > n2 Y n1 > n3 Entonces
			Escribir "n1 es mayor";
		SiNo
			Si n2 > n3 Entonces
				Escribir "el numero mayor es el 2";
			SiNo
				Escribir "el numero 3 es mayor";
			FinSi
		FinSi
		
		
FinProceso

	

